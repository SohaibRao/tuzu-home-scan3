{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    }
  ],
  "functions": {
    "src/app/api/upload/route.ts": {
      "maxDuration": 60,
      "memory": 2048
    }
  }
}
